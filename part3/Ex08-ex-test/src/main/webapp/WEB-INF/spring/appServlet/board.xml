<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="contentHandler" class="com.study.spring.command.BContentCommand"></bean>
	<bean id="deleteHandler" class="com.study.spring.command.BDeleteCommand"></bean>
	<bean id="listHandler" class="com.study.spring.command.BListCommand"></bean>
	<bean id="modifyHandler" class="com.study.spring.command.BModifyCommand"></bean>
	<bean id="replyHandler" class="com.study.spring.command.BReplyCommand"></bean>
	<bean id="replyViewHandler" class="com.study.spring.command.BReplyViewCommand"></bean>
	<bean id="writeHandler" class="com.study.spring.command.BWriteCommand"></bean>
</beans>
